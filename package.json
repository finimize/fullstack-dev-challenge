{
  "name": "fullstack-dev-challenge",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.1.0",
    "express": "4.13.3",
    "mocha": "^3.4.2"
  },
  "scripts": {
