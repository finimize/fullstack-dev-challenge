{
  "name": "fullstack-dev-challenge",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "express": "4.17.1"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.ts",
    "types": "tsc",
    "client": "node scripts/start-client.js",
    "install": "cd client && yarn install && cd ..",
    "test": "jest --forceExit"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.14.14",
    "@types/supertest": "^2.0.11",
    "babel-cli": "^6.26.0",
    "concurrently": "5.3.0",
    "jest": "26.6.0",
    "mocha": "^8.2.1",
    "supertest": "^6.2.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/client/"
    ]
  }
}