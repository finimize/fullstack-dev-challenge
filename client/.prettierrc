{
    "$schema": "http://json.schemastore.org/prettierrc",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "endOfLine": "lf",
    "jsxSingleQuote": true,
    "arrowParens": "always"
}